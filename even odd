import java.util.*;
public class Main{
    public static void main(String[]ags){
        Scanner input = new Scanner(System.in);
        System.out.println("enter number");
        int num = input.nextInt();
        if(num %2==0){
            System.out.println("even");
        }
     else{
         System.out.println("odd");
            }
            
        
    }
}
